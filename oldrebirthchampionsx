    CurrentValue = false,
    Flag = "WorldBoost", 
    Callback = function(Value)
      getgenv().Toggle = Value
      while getgenv().Toggle == true do wait()
               if getgenv().Toggle == true then
          game:GetService("ReplicatedStorage").Events.WorldBoost:FireServer(BestBoost) 
          game:GetService("ReplicatedStorage").Events.StoreWorldBoost:FireServer(BestBoost)
        else 
       end
      end
    end,
  })
  
  
  local WhiteScreen = Tab8:CreateToggle({
    Name = "White Screen [ CPU Saver ]",
    CurrentValue = false,
    Flag = "WhiteScreen", 
    Callback = function(Value)
      getgenv().Toggle = Value
      while getgenv().Toggle == true do wait()
              if getgenv().Toggle == true then
          game:GetService("RunService"):Set3dRenderingEnabled(false)
          setfpscap(10)
        else 
          game:GetService("RunService"):Set3dRenderingEnabled(true)
          setfpscap(360)
        end
      end
    end,
  })
  
