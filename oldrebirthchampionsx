  function potion(potion, amount)
  
  local args = {
      [1] = potion,
      [2] = amount
  }
  
  game:GetService("ReplicatedStorage").Events.Potion:FireServer(unpack(args))
  end
  
  local potionchoice;
  local PotionChoice = Tab5:CreateDropdown({
    Name = "Potion",
    Options = {"x2Clicks", "x2Gems", "x2Luck", "x2Rebirths", "x2PetXP", "x2HatchSpeed"},
    CurrentOption = "Select Potion",
    Flag = "Egg Choice",
    Callback = function(PetChoice)
    potionchoice = PetChoice
    end    
  })	
  
  local Selectamount;
  local SelectAmount = Tab5:CreateDropdown({
    Name = "Select The Amount",
    Options = {"1", "10", "100"},
    CurrentOption = "Select Mode",
    Flag = "Mode Choice",
    Callback = function(Option)
      if Option == "1" then
        Selectamount = 1;
      elseif Option == "10" then
        Selectamount = 10;
      elseif Option == "100" then
        Selectamount = 100;
      end
      end    
  })
  
  local LabelPotion = Tab5:CreateLabel("Must Set Potion Choice and Amount First")
  
