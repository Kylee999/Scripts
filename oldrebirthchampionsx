    Flag = "Egg Hunt", 
    Callback = function(Value)
      for i, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.EggHuntFrame:GetChildren()) do
        if v.Name =="Close" then
           v.Visible = false
        end
      end
      getgenv().Toggle = Value
      while getgenv().Toggle == true do wait()
               if getgenv().Toggle == true then game:GetService("Players").LocalPlayer.PlayerGui.MainUI.EggHuntFrame.Visible = true else
                    if getgenv().Toggle == false then game:GetService("Players").LocalPlayer.PlayerGui.MainUI.EggHuntFrame.Visible = false
            game:GetService("Players").LocalPlayer.PlayerGui.MainUI.EggHuntFrame.Close.Visible = false
            game:GetService("Players").LocalPlayer.PlayerGui.MainUI.EggHuntFrame.Close.Visible = false
    end
  end
  end
  
    end,
  })
  
