  local AutoChristmas = Tab7:CreateButton({
    Name = "Auto Fully Christmas Hunt",
    Callback = function()
    local part = Instance.new("Part", game.Workspace) 
    part.Anchored = true
    part.Size = Vector3.new(5,0.5,5)
    local hrp = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    ChristmasLabel:Set("Status : Doing First Puzzle")
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(345, -52.7356339, 153.5, 1, 0, 0, 0, 1, 0, 0, 0, 1)
  part.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - 3.25, hrp.Position.Z)   
  wait(1)
  fireproximityprompt(game:GetService("Workspace").Scripts.Christmas.Hunt.Hamster.Main.Main.ProximityPrompt)
  wait(8)
  ChristmasLabel:Set("Status : Doing Second Puzzle")
  firesignal(game:GetService("Players").dacoolmet.PlayerGui.MainUI.ShopFrame.Top.Holder.ScrollingFrame.Boosts.BoostBundle.Top.Ornament.Click.MouseButton1Click)
  part.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - 3.25, hrp.Position.Z)   
  wait(8)
  ChristmasLabel:Set("Status : Doing Third Puzzle")
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-430.041412, 28.8067989, 215.202301, 0.88286835, -0, -0.469620556, 0, 1, -0, 0.469620556, 0, 0.88286835)
  part.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - 3.25, hrp.Position.Z)   
  wait(1)
  fireproximityprompt(game:GetService("Workspace").Scripts.Christmas.Hunt.Tree.Main.Main.ProximityPrompt)
  wait(8)
  ChristmasLabel:Set("Status : Doing Fourth Puzzle")
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1036.16553, -36.2134018, 209.909912, 0.235349417, 0, 0.971910775, 0, 1, 0, -0.971910775, 0, 0.235349417)
  part.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - 3.25, hrp.Position.Z)   
