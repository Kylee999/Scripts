  local AutoCollectInfo = Tab7:CreateSection("Main Farming")
  
  
  local ChristmasCollect = Tab7:CreateToggle({
    Name = "Auto Collect Presents",
    CurrentValue = false,
    Flag = "AutoCollect", 
    Callback = function(Value)
      getgenv().ChristmasCollect = Value
          while getgenv().ChristmasCollect do
          wait(0.2)
          for i,v in pairs(game:GetService("Workspace").Scripts.PresentsCollect.Storage:GetDescendants()) do
            if (v.Name == "TouchInterest" and v.Parent) then
            firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 0)
            wait()
            break;
           end
          end 
          end
    end,
  })
  
